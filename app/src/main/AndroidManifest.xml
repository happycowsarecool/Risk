<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sheda.risk">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".RiskActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PopUpWindow"></activity>
        <activity
            android:name=".RiskBoardActivity"
            android:label="@string/title_activity_risk_board"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HowToActivity"
            android:label="@string/title_activity_how_to"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PreparationActivity"
            android:label="@string/title_activity_preparation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ArmiesUnitsActivity"
            android:label="@string/title_activity_armies_units"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AttackingDefendingActivity"
            android:label="@string/title_activity_attacking_defending"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FortifyingActivity"
            android:label="@string/title_activity_fortifying"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CardActivity"
            android:label="@string/title_activity_card"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EndScreenActivity"
            android:label="@string/title_activity_end_screen"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>